import redis

def save2redis(csvpath):
    for root,dirs,files in os.walk(linkpath):
    for f in files:
        if os.path.splitext(f)[1]!= '.csv':
            continue
        linkfile = os.path.join(root,f)
        with open(linkfile,'rb') as csvfile:
            reader = csv.reader(csvfile)
            num = 0
            for line in reader:
                num = num +1
                if num <= 2:
                    continue
                url = line[3]
                urls.append(url)


